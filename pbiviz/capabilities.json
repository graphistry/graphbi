            "displayNameKey": "Visual_EdgeProperty",
            "name": "EdgeProperty",
            "kind": "Grouping",
            "displayName": "Edge properties"
        }
    ],
    "sorting": {
        "default": {}
    },
    "drilldown": {
        "roles": [
            "Source",
            "Target"
        ]
    },
    "supportsLandingPage": true,
    "supportsEmptyDataView": true,
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "Source": {
                        "min": 1,
                        "max": 1
                    },
                    "Destination": {
                        "min": 1,
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "Source"
                            }
                        },
                        {
                            "for": {
                                "in": "Destination"
                            }
                        },
                        {
                            "for": {
                                "in": "EdgeProperty"
                            }
                        },
                        {
                            "for": {
                                "in": "SourceProperty"
                            }
                        },
                        {
                            "for": {
                                "in": "DestinationProperty"
                            }
                        }
                    ]
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "EdgeWeight"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "advancedEditModeSupport": 1,
